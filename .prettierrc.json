{
	"printWidth": 120,
	"tabWidth": 4,
	"useTabs": false,
	"semi": true,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"endOfLine": "lf",
	"importOrder": [
        "^@/DataViews/(.*)$",
        "^@/elements/(.*)$",
        "^@/api(.*)$",
        "^[./]"
    ],
    "plugins": ["prettier-plugin-tailwindcss"]
}
